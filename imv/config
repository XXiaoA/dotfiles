[options]
overlay = true
overlay_font = Maple Mono SC NF:10

[binds]
<Shift+less> = rotate by -90
<Shift+greater> = rotate by 90
<equal> = zoom 1
0 = zoom actual
1 = zoom actual
2 = zoom actual; zoom 17.672987685129698
3 = zoom actual; zoom 28.011022756637367
4 = zoom actual; zoom 35.345975370259396
5 = zoom actual; zoom -17.672987685129698
w = scaling full
p = prev
n = next
y = exec echo $imv_current_file

